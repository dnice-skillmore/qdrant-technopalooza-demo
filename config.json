{
  "service": {
    "rest_api_port": 6333,
    "grpc_port": 6334,
    "max_request_size_mb": 32,
    "use_http": true,
    "max_concurrent_searches": 4
  },
  "storage": {
    "path": "/qdrant/storage",
    "optimizer_threshold": 100000,
    "compression": {
      "threshold": 1000,
      "ratio": 0.7
    }
  },
  "cluster": {
    "enabled": false,
    "p2p_port": 6335,
    "discovery_timeout_secs": 60,
    "p2p_bind": "0.0.0.0"
  },
  "logger": {
    "level": "INFO",
    "file": "/qdrant/logs/qdrant.log"
  },
  "wal": {
    "wal_path": "/qdrant/wal",
    "wal_capacity_mb": 1024
  },
  "hnsw": {
    "m": 16,
    "ef_construct": 200,
    "full_scan_threshold": 100
  }
}
